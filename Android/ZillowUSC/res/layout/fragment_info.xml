                    <TextView
                        android:id="@+id/taxAYearV"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="end"
                        android:gravity="center_vertical"
                        android:paddingRight="5dip"
                        android:text="" />
                </TableRow>

                <TableRow
                    android:layout_width="0dp"
                    android:layout_height="1dip"
                    android:background="@drawable/border"
                    android:minHeight="2dp" />

                <TableRow
                    android:id="@+id/tableRow10"
                    android:layout_width="0dp"
                    android:background="#FFFFFF"
                    android:minHeight="25dp" >

                    <TextView
                        android:id="@+id/taxA"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:paddingLeft="5dip"
                        android:text="Tax Assessment" />

                    <TextView
                        android:id="@+id/taxAV"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="end"
                        android:gravity="center_vertical"
                        android:paddingRight="5dip"
                        android:text="" />
                </TableRow>

                <TableRow
                    android:layout_width="0dp"
                    android:layout_height="1dip"
                    android:background="@drawable/border"
                    android:minHeight="2dp" />

                <TableRow
                    android:id="@+id/tableRow11"
                    android:layout_width="0dp"
                    android:background="#EAF7FC"
                    android:minHeight="25dp" >

                    <TextView
                        android:id="@+id/lsp"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:paddingLeft="5dip"
                        android:text="Last Sold Price" />

                    <TextView
                        android:id="@+id/lspV"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="end"
                        android:gravity="center_vertical"
                        android:paddingRight="5dip"
                        android:text="" />
                </TableRow>

                <TableRow
                    android:layout_width="0dp"
                    android:layout_height="1dip"
                    android:background="@drawable/border"
                    android:minHeight="2dp" />

                <TableRow
                    android:id="@+id/tableRow12"
                    android:layout_width="0dp"
                    android:background="#FFFFFF"
                    android:minHeight="25dp" >

                    <TextView
                        android:id="@+id/lsd"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:paddingLeft="5dip"
                        android:text="Last Sold Date" />

                    <TextView
                        android:id="@+id/lsdV"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="end"
                        android:gravity="center_vertical"
                        android:paddingRight="5dip"
                        android:text="" />
                </TableRow>

                <TableRow
                    android:layout_width="0dp"
                    android:layout_height="1dip"
                    android:background="@drawable/border"
                    android:minHeight="2dp" />

                <TableRow
                    android:id="@+id/tableRow13"
                    android:layout_width="0dp"
                    android:background="#EAF7FC"
                    android:minHeight="25dp" >

                    <TextView
                        android:id="@+id/propEst"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:maxWidth="35dp"
                        android:paddingLeft="5dip"
                        android:text="Zestimate Property Estimate as of" />

                    <TextView
                        android:id="@+id/propEstV"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="end"
                        android:gravity="center_vertical"
                        android:paddingRight="5dip"
                        android:text="" />
                </TableRow>

                <TableRow
                    android:layout_width="0dp"
                    android:layout_height="1dip"
                    android:background="@drawable/border"
                    android:minHeight="2dp" />

                <TableRow
                    android:id="@+id/tableRow14"
                    android:layout_width="0dp"
                    android:background="#FFFFFF"
                    android:minHeight="25dp" >

                    <TextView
                        android:id="@+id/ovlChange"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:paddingLeft="5dip"
                        android:text="30 days Overall Change" />

                    <TextView
                        android:id="@+id/ovlChangeV"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="end"
                        android:gravity="center_vertical"
                        android:paddingRight="5dip"
                        android:text="" />
                </TableRow>

                <TableRow
                    android:layout_width="0dp"
                    android:layout_height="1dip"
                    android:background="@drawable/border"
                    android:minHeight="2dp" />

                <TableRow
                    android:id="@+id/tableRow15"
                    android:layout_width="0dp"
                    android:background="#EAF7FC"
                    android:minHeight="25dp" >

                    <TextView
                        android:id="@+id/atpr"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:paddingLeft="5dip"
                        android:text="All Time Property Range" />

                    <TextView
                        android:id="@+id/atprV"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="end"
                        android:gravity="center_vertical"
                        android:paddingRight="5dip"
                        android:text="" />
                </TableRow>

                <TableRow
                    android:layout_width="0dp"
                    android:layout_height="1dip"
                    android:background="@drawable/border"
                    android:minHeight="2dp" />

                <TableRow
                    android:id="@+id/tableRow16"
                    android:layout_width="0dp"
                    android:background="#FFFFFF"
                    android:minHeight="25dp" >

                    <TextView
                        android:id="@+id/rentEst"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:maxWidth="35dp"
                        android:paddingLeft="5dip"
                        android:text="Rent Zestimate Valuation as of" />

                    <TextView
                        android:id="@+id/rentEstV"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="end"
                        android:gravity="center_vertical"
                        android:paddingRight="5dip"
                        android:text="" />
                </TableRow>

                <TableRow
                    android:layout_width="0dp"
                    android:layout_height="1dip"
                    android:background="@drawable/border"
                    android:minHeight="2dp" />

                <TableRow
                    android:id="@+id/tableRow17"
                    android:layout_width="0dp"
                    android:background="#EAF7FC"
                    android:minHeight="25dp" >

                    <TextView
                        android:id="@+id/rentCh"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:paddingLeft="5dip"
                        android:text="30 Days Rent Change" />

                    <TextView
                        android:id="@+id/rentChV"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="end"
                        android:gravity="center_vertical"
                        android:paddingRight="5dip"
                        android:text="" />
                </TableRow>

                <TableRow
                    android:layout_width="0dp"
                    android:layout_height="1dip"
                    android:background="@drawable/border"
                    android:minHeight="2dp" />

                <TableRow
                    android:id="@+id/tableRow18"
                    android:layout_width="0dp"
                    android:background="#FFFFFF"
                    android:minHeight="25dp" >

                    <TextView
                        android:id="@+id/atrr"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:paddingLeft="5dip"
                        android:text="All Time Rent Range" />

                    <TextView
                        android:id="@+id/atrrV"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="end"
                        android:gravity="center_vertical"
                        android:paddingRight="5dip"
                        android:text="" />
                </TableRow>

                <TableRow
                    android:layout_width="0dp"
                    android:layout_height="1dip"
                    android:background="@drawable/border"
                    android:minHeight="2dp" />
            </TableLayout>
        </ScrollView>
    </LinearLayout>

    <TextView
        android:id="@+id/section_label"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="#FFFFFF"
        android:gravity="center_vertical|center_horizontal" />

</RelativeLayout>